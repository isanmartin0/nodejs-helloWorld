{
  "name": "nodejs-helloWorld",
  "version": "1.0.0",
  "description": "Hello World on Node js",
  "main": "app.js",
  "scripts": {
    "start": "node src/api.js",
    "test": "jest"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/.vscode/**",
      "!**/.git/**"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 20
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isanmartin0/nodejs-helloWorld.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isanmartin0/nodejs-helloWorld/issues"
  },
  "homepage": "https://github.com/isanmartin0/nodejs-helloWorld#readme",
  "dependencies": {
    "express": "^4.16.3",
    "jade": "^1.11.0",
    "joi": "^13.1.0"
  },
  "devDependencies": {
    "jest": "^18.1.0",
    "jest-sonar-reporter": "^2.0.0"
  }
}
