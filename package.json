{
  "name": "keedio-nodejs-helloworld",
  "version": "1.0.5",
  "description": "Hello World on Node js",
  "main": "server/app.js",
  "scripts": {
    "start": "scripts/run.sh",
    "test": "jest",
    "script_prueba": "node server/app.js"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "verbose": true,
    "collectCoverage": true,
    "testRegex": "mytests/.*\\.(js|jsx)$",
    "collectCoverageFrom": [
      "server/**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/.vscode/**",
      "!**/.git/**"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 20
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isanmartin0/nodejs-helloWorld.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isanmartin0/nodejs-helloWorld/issues"
  },
  "homepage": "https://github.com/isanmartin0/nodejs-helloWorld#readme",
  "dependencies": {
    "express": "^4.16.2",
    "jade": "^1.11.0",
    "joi": "^13.1.0",
    "@google/chatbase": "^1.1.1",
    "babel-core": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "consign": "^0.1.6",
    "dialogflow": "^0.1.0",
    "dotenv": "^5.0.0",
    "iban": "0.0.10",
    "json-templater": "^1.2.0",
    "kafka-node": "^2.3.2",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "node-cache": "^4.2.0",
    "npm": "^5.7.1",
    "redis": "^2.8.0",
    "request-promise": "^4.2.2",
    "socks5-https-client": "^1.2.1",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^2.0.14",
    "uid-generator": "^1.0.0",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "gulp-if": "^2.0.0",
    "gulp-jscs": "^3.0.2"
    "gulp-jshint": "^1.12.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-nodemon": "^2.0.4",
    "gulp-print": "^2.0.1",
